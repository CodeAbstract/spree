<% content_for :page_title do %>
  <%= page_header_back_button spree.admin_shipping_methods_url %>
  <%= Spree.t(:new_shipping_method) %>
<% end %>

<div data-hook="admin_shipping_method_new_form_header">
  <%= render partial: 'spree/admin/shared/error_messages', locals: { target: @shipping_method } %>
</div>

<div data-hook="admin_shipping_method_new_form">
  <%= form_for [:admin, @shipping_method] do |f| %>
    <%= render 'form', f: f %>
    <%= render 'spree/admin/shared/new_resource_links' %>
  <% end %>
</div>
